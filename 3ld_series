#include<stdio.h>
#include<math.h>

void main(){
    float a=0.,b=2*M_PI,x,delta_x=1.e-2;
    //float y;
    printf("\tx\t\tsin(x)\t\tsin\'(x)\n");
    x=a;
    while(x<b){
        //y=asin(x);
        printf("%10.2f\t%10.2f\t%10.2f\n",x,asin(x),(asin(x+delta_x)-asin(x))/delta_x);
        //printf("%10.2f\t%10.2f\",x,y);
        x+=delta_x;//x+x+delta_x;
    }
}
